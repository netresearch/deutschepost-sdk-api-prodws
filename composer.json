{
    "name": "deutschepost/sdk-api-prodws",
    "description": "Deutsche Post DHL Group Shipping Products API SDK",
    "keywords": [
        "php",
        "sdk",
        "dpdhl",
        "dhl",
        "internetmarke",
        "prodws",
        "soap"
    ],
    "license": "MIT",
    "authors": [
        {
            "email": "plugins@netresearch.de",
            "name": "Netresearch DTT GmbH",
            "role": "Manufacturer",
            "homepage": "https://www.netresearch.de/"
        }
    ],
    "require": {
        "php": "^7.2.0 || ^8.0.0",
        "ext-dom": "*",
        "ext-soap": "*",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0.0 || ^9.0.0",
        "phpstan/phpstan": "^1.5.0",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "DeutschePost\\Sdk\\ProdWS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DeutschePost\\Sdk\\ProdWS\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.1.0.x-dev"
        }
    }
}
